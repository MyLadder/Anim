<?xml version="1.0" encoding="utf-8"?>
<resources>






    <style name="MenuDialogFragmentStyle">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:colorForeground">@android:color/transparent</item>
        <item name="android:colorForegroundInverse">@android:color/transparent</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>


    <style name="main_title_left">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="main_title_right">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">normal</item>
    </style>


    <style name="sub_title">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#8f000000</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">normal</item>
    </style>


    <style name="option_label">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">normal</item>
    </style>


    <style name="option_description">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#8f000000</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">normal</item>
    </style>

</resources>